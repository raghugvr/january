pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                echo "Hello welcome to Jenkins"
            }
        }
        stage('Test') { 
            steps {
                echo "Test the file"
            }
        }
        stage('Deploy') { 
            steps {
                echo "You have done your first pipeline"
            }
         stage('Done') { 
            steps {
                echo "You have done your first pipeline"
            }   
        }
    }
}
