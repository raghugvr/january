pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                echo "Hello welcome to Jenkins Venkat"
            }
        }
        stage('Test') { 
            steps {
                echo "Test the fileee"
            }
        }
        stage('Deploy') { 
            steps {
                echo "You have done your first pipeline"
            }  
        }
    }
 }
